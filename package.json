  "dependencies": {
    "@nhl-api/players": "^3.1.4",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "mongoose": "^6.1.5",
    "node-fetch": "^3.1.0"
